{
  sourceMaps: true,
  presets: [
      ['@babel/preset-env', {
          targets: {
              ie: 11,
              browsers: 'last 2 versions'
          },
          useBuiltIns: 'usage'
      }],
      ["@babel/preset-flow"],
      ["@babel/preset-react"]
  ],
  ignore: [ 'node_modules' ],
  plugins: [
    ["babel-plugin-root-import", {
      "rootPathPrefix": "@"
    }]
  ]
}